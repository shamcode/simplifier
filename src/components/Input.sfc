<template>
    <input
        id={{id}}
        placeholder={{placeholder}}
        tabindex="{{tabindex}}"
        name={{name}}
    />
</template>

<script>
    import { mergeWithTemplate, options } from 'sham-ui-macro/babel.macro';

    @mergeWithTemplate
    class Input extends Template {
        @options placeholder = '';
        @options tabindex = 1;
    }

    export default Input;
</script>